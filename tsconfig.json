{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@config/*": ["src/config/*"],
      "@scenes/*": ["src/scenes/*"],
      "@services/*": ["src/services/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@default/*": ["src/*"],
      "@utils/*": ["src/utils/*"],
      "@managers/*": ["src/managers/*"],
    }
  },
  "include": ["src/**/*"]
}